<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:id="@+id/listview_background_shape">
        <stroke android:width="6dp" android:color="#ddffffff" />
        <padding android:left="2dp"
            android:top="2dp"
            android:right="2dp"
            android:bottom="2dp" />
        <corners android:radius="8dp" />
    </shape>
</item>
<item>
    <shape android:id="@+id/listview_foreground_shape">
        <stroke android:width="6dp" android:color="#ddffffff" />
        <padding android:left="2dp"
            android:top="2dp"
            android:right="2dp"
            android:bottom="2dp" />
        <corners android:radius="8dp" />
    </shape>
</item>
</layer-list>